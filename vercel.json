{
  "version": 2,
  "functions": {
    "Admin-dashboard/**/*.php": {
      "runtime": "php@8.1"
    },
    "Applicant-dashboard/**/*.php": {
      "runtime": "php@8.1"
    },
    "Staff-dashboard/**/*.php": {
      "runtime": "php@8.1"
    },
    "**/*.php": {
      "runtime": "php@8.1"
    }
  },
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/Admin-dashboard/$1"
    },
    {
      "source": "/applicant/(.*)",
      "destination": "/Applicant-dashboard/$1"
    },
    {
      "source": "/staff/(.*)",
      "destination": "/Staff-dashboard/$1"
    }
  ]
}
