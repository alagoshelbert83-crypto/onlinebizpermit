{
  "version": 2,
  "functions": {
    "**/*.php": {
      "runtime": "php@8.1"
    }
  },
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/Admin-dashboard/$1"
    },
    {
      "source": "/applicant/(.*)",
      "destination": "/Applicant-dashboard/$1"
    },
    {
      "source": "/staff/(.*)",
      "destination": "/Staff-dashboard/$1"
    },
    {
      "source": "/Admin-dashboard/(.*)",
      "destination": "/Admin-dashboard/$1"
    },
    {
      "source": "/Applicant-dashboard/(.*)",
      "destination": "/Applicant-dashboard/$1"
    },
    {
      "source": "/Staff-dashboard/(.*)",
      "destination": "/Staff-dashboard/$1"
    }
  ]
}
